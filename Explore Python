# Set

myset = {1 , 2 , 3 , 4 , 5 }
print(myset)

s1 = {'cherry' , 'banana' , 'apple'}
s2 = {1 , 2 , 3 , 4 , 5}
s3 = {False , True }
print(s1)
print(s2)
print(s3)


#Sorting

nums = [1, 5, 3, 4, 2, 10, 6, 8, 7, 9]
nums.sort()
print('List in Dscending Order: ', nums)

nums.sort(reverse=True)
print('List in Descending Order: ', nums)
