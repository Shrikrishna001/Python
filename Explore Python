Created on Sun Jan 23 19:58:39 2022

@author: DELL
"""

'''Creating a Function'''

#Def
def my_function():
  print("Hello from a function")

my_function()
  
#Arguments
def OS(osname):
  print(osname + " OS")

OS("Windows")
OS("Mac")
OS("Linux")

def add_sub(x , y):
    a = (x + y)
    b = (x - y)
    
    return a , b

result1, result2 = add_sub(20, 10)
print(result1 ,result2)

''' Dictionary method'''
car = {
       "brand": "Ford",
   }
car.clear()
print(car)

car = {
       "brand": "BMW"
       }
car.copy()
print(car)

x = ('key 1' , 'key 2' , 'key 3')
thisdict = dict.fromkeys(x)

print(thisdict)


